+int32_t Gt9xxTouchDriverOpen(struct HdfDeviceObject *object)
+{
+    if (object == NULL) {
+        HDF_LOGE("%s: param is null", __func__);
+        return HDF_ERR_INVALID_PARAM;
+    }
+    static struct IDeviceIoService service = {
