 #define I2C_BUS_MAX 8
 
+#define I2C_READ_MSG_NUM   2
+#define I2C_WRITE_MSG_NUM  1
+#define I2C_REG_BUF_LEN    4
+#define I2C_BYTE_MASK      0xFF
+#define I2C_BYTE_OFFSET    8
+
+
+
