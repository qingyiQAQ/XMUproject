+/* IO config for int-pin and I2C-pin */
+#define I2C6_DATA_REG_ADDR 0x114f004c
+#define I2C6_CLK_REG_ADDR 0x114f0048
+#define INR_REG_ADDR 0x10ff002C
+#define RST_REG_ADDR 0x10ff0030
+#define I2C_REG_CFG 0x403
