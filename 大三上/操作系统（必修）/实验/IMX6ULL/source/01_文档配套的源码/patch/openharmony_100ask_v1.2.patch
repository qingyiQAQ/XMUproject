+static void Imx6ull_lcd_controller_enable(void)
+{	
+	LCDIF->CTRL |= 1<<0; /* 6ULLLCD */
+}
+static void Imx6ull_lcd_controller_disable(void)
+{
+	LCDIF->CTRL &= ~(1<<0); /* 6ULLLCD */
+}
+
+static void Imx6ull_lcd_io_init(void)
+{
+	unsigned int* p_tmp;
+	unsigned int i = 0 ;
+
+	p_tmp = (unsigned int*)(SW_MUX);
+	for(i = 0 ; i < 29 ; i++)		       /*29*/
+	{
+		*p_tmp = 0;		   /*290LCD*/
+		p_tmp++;
+	}	
+	/*LCDbit
+     * [16]     :   0  :   
+	 * [15:14]  :   0  : 22K
+	 * [13]     :   0  : Keeper
+	 * [12]     :   0  : KEEP
+	 * [11]     :   0  : 
+	 * [7:6]    :   2  : 100Mhz
+	 * [5:3]    :   7  : R0/7 
+	 * [0]      :   1  : 
+	 */
+	
+    p_tmp = (unsigned int*)(SW_PAD);
+	for(i = 0 ; i < 29 ; i++)	
+	{
+		*p_tmp = 0;
+		*p_tmp |= 0xB9;
+		 p_tmp++;
+	}
+
+	/* GPIO */
+
+	
+	*GPIO1_IO08_PAD     = 0xB9  ;							/**/
+
+    *GPIO1_GDIR        |= (1<<8);                             /*GPIO*/
+
+	*GPIO1_DR          |= (1<<8);                             /**/
+
+
+}
