+ */
+
+#ifndef I2C_USER_H
+#define I2C_USER_H
+
+#include "hdf_base.h"
+
+enum I2cIoctlCmd {
+    IOCTL_RETRIES      = 0x0701,
+    IOCTL_TIMEOUT      = 0x0702,
+    IOCTL_SLAVE        = 0x0703,
+    IOCTL_SLAVE_FORCE  = 0x0706,
+    IOCTL_TENBIT       = 0x0704,
+    IOCTL_FUNCS        = 0x0705,
+    IOCTL_RDWR         = 0x0707,
+    IOCTL_PEC          = 0x0708,
+    IOCTL_SMBUS        = 0x0720,
+    IOCTL_16BIT_REG    = 0x0709,  /* 16BIT REG WIDTH */
+    IOCTL_16BIT_DATA   = 0x070a,  /* 16BIT DATA WIDTH */
+};
+
+enum I2cMsgFlag {
+    I2C_M_RD           = 0x0001,
