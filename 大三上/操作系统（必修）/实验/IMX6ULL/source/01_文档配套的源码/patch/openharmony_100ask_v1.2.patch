+        if (UART0 == uart_num) {} \
+        else if (UART1 == uart_num) { \
+        WRITE_UINT16(0x01, IO_MUX_REG_BASE + 0x07C); \
+        WRITE_UINT16(0x01, IO_MUX_REG_BASE + 0x084); \
