+const struct file_operations_vfs uartdev_fops = {
+    .open   = uartdev_open,
+    .close  = uartdev_release,
+    .read   = uartdev_read,
+    .write  = uartdev_write,
+    .seek   = NULL,
+    .ioctl  = uartdev_ioctl,
+    .mmap   = uartdev_map,
+#ifndef CONFIG_DISABLE_POLL
+    .poll   = uartdev_poll,
+#endif
+    .unlink = NULL,
+};
