+
+void myputhex(unsigned int val)
+{
+	/* 0x76543210 */
+	int i, j;
+
+	myputs("0x");
+	for (i = 7; i >= 0; i--)
+	{
+		j = (val >> (i*4)) & 0xf;
+		if ((j >= 0) && (j <= 9))
+			uart_putc_virt('0' + j);
+		else
+			uart_putc_virt('A' + j - 0xA);
+	}	
+}
