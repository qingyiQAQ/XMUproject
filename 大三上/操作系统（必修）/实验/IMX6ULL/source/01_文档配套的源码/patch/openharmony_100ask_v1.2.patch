+#define SW_MUX			  (SW_MUX_CTL_Type*)SW_MUX_PART_LCD_BASE
+#define SW_PAD            (SW_PAD_CTL_Type*)SW_PAD_PART_LCD_BASE
+#define CCM               ((CCM_Type *)CCM_BASE)
