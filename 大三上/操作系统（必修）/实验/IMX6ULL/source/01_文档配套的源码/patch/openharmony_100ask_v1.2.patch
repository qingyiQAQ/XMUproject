+/* HIFMC REG */
+/*****************************************************************************/
+#define FMC_CFG                             0x00
+#define FMC_CFG_SPI_NAND_SEL(_type)         (((_size) & 0x3) << 11)
+#define FMC_CFG_SPI_NOR_ADDR_MODE(_mode)    ((_mode) << 10)
+#define FMC_CFG_BLOCK_SIZE(_size)           (((_size) & 0x3) << 8)
