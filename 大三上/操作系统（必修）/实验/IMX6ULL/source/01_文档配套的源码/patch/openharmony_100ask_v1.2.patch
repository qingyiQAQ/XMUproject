+STATIC VOID UartPutStr(UINTPTR base, const CHAR *s, UINT32 len)
+{
+    UINT32 i;
+    for (i = 0; i < len; i++) {
+        if (*(s + i) == '\n') {
+			UartPutcReg(base, *"\r");
+        }
+		UartPutcReg(base, *(s + i));
+    }
