+    INDEV_TYPE_KEY,
+    INDEV_TYPE_KEYBOARD,
+    INDEV_TYPE_MOUSE,
+    INDEV_TYPE_BUTTON,
+    INDEV_TYPE_CROWN,
+    INDEV_TYPE_ENCODER, /* Encoder with specific function or event */
+    INDEV_TYPE_MAX,
