+/*STM32MP157<<53.7 Universal synchronous/asynchronous receiver transmitter (USART/UART)>>2655USART4,*/
+typedef struct {
+  volatile unsigned int  USART_CR1;          /**< USART control register 1,                 offset: 0x00 	 1            0x00  */
+  volatile unsigned int  USART_CR2;          /**< USART control register 2,                 offset: 0x04 	 2            0x04  */
+  volatile unsigned int  USART_CR3;          /**< USART control register 3,                 offset: 0x08 	 3            0x08  */
+  volatile unsigned int  USART_BRR;          /**< USART Baud Rate register,                 offset: 0x0C                  0x0C  */
+  volatile unsigned int  USART_GTPR;         /**< USART guard time and prescaler register,  offset: 0x10      0x10  */
+  volatile unsigned int  USART_RTOR;         /**< USART receiver timeout register,          offset: 0x14                0x14  */
+  volatile unsigned int  USART_RQR;          /**< USART request register,                   offset: 0x18                    0x18  */
+  volatile unsigned int  USART_ISR;          /**< USART interrupt and status register,      offset: 0x1C              0x1C  */
+  volatile unsigned int  USART_ICR;          /**< USART interrupt flag clear register ,     offset: 0x20            0x20  */
+  volatile unsigned int  USART_RDR;          /**< USART receive data register,              offset: 0x24                0x24  */
+  volatile unsigned int  USART_TDR;          /**< USART transmit data register,             offset: 0x28                0x28  */
+  volatile unsigned int  USART_PRESC;        /**< USART prescaler register,                 offset: 0x2C                0x2C  */
+} UART_Type;
