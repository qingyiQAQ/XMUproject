+enum hifmc_ecc_reg {
+    hifmc_ecc_0bit   = 0x00,
+    hifmc_ecc_8bit   = 0x01,
+    hifmc_ecc_16bit  = 0x02,
+    hifmc_ecc_24bit  = 0x03,
+    hifmc_ecc_28bit  = 0x04,
+    hifmc_ecc_40bit  = 0x05,
+    hifmc_ecc_64bit  = 0x06,
+};
+
