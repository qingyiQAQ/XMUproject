+#define mtd_trace(debug, msg...) do { \
+    if (debug == ENABLE) { \
+        dprintf("%s:%d: ", __func__, __LINE__); \
+        dprintf(msg); \
+        dprintf("\n"); \
+    } \
+} while (0)
