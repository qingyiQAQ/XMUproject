+    msg[0].addr = client->i2cCfg.addr;
+    msg[0].flags = 0;
+    msg[0].len = len;
+    msg[0].buf = writeData;
+
+    if (I2cTransfer(client->i2cHandle, msg, I2C_WRITE_MSG_NUM) != I2C_WRITE_MSG_NUM) {
+        HDF_LOGE("%s: i2c write err", __func__);
+        return HDF_FAILURE;
+    }
+    return HDF_SUCCESS;
+}
