+ * Description: ft6236 touch driver implement.
+ * Author: zhaihaipeng
+ * Create: 2020-07-25
+ */
+
+#include <stdlib.h>
+#include <asm/io.h>
+#include <fs/fs.h>
+#include <fs_poll_pri.h>
+#include <los_queue.h>
+#include <poll.h>
+#include <user_copy.h>
